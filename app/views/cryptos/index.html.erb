<div class="container mt40" style="height: 100vh">
  <div class="row">
    <% @cryptos.each do |crypto|%>
      <div class="col-xs-12 col-md-4 col-sm-3"">
        <div class="card-index-wagon">
          <div class="card-category-wagon">
            <img src="<%= crypto.photo_url %>", width="60">
          </div>
          <div class="card-description-wagon text-center">
            <%= crypto.name%>
          </div>
            <%= link_to crypto_path(crypto), :class => "card-link-wagon" do %>
              <div class="text-center crypto-title">
              </div>
            <% end %>
        </div>
      </div>
    <% end %>
